<%- include('templates/header'); -%>

<div class='container'>
    <div class='jumbotron'>
        <h1>Search Result</h1>  
        <button class='btn btn-info' onclick='location.href="/"'>Search Again</button>
    </div>
    <ul id='book-list'>
            <li>
                <img class='poster' src=<%= data.cover.medium%> >
                <div>
                    <p> Author: <%= data.authors[0].name %></p>
                    <p> Publisher: <%= data.publishers[0].name %></p>
                    <p> Date Published: <%= data.publish_date %></p>
                </div>
            </li>
    </ul>
</div>


<%- include('templates/footer'); -%>